{
  "name": "@adaptivelink/pops",
  "version": "0.5.9",
  "description": "Points of Presence geolocation",
  "main": "dist/bundle.js",
  "types": "main.d.ts",
  "type": "module",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adaptive/pops.git"
  },
  "bugs": {
    "url": "https://github.com/adaptive/pops/issues"
  },
  "homepage": "https://github.com/adaptive/pops#readme",
  "author": "Hugo Romano <adaptive@hey.com>",
  "license": "MIT",
  "keywords": [
    "cdn",
    "alibaba",
    "arubacloud",
    "atlanticnet",
    "aws",
    "azurecdn",
    "backblaze",
    "bunnycdn",
    "cachefly",
    "cdn77",
    "cloudflare",
    "cloudfront",
    "cloudsigma",
    "deno",
    "digitalocean",
    "equinix",
    "fastly",
    "flexential",
    "google",
    "heroku",
    "hetzner",
    "hivelocity",
    "hostwinds",
    "imperva",
    "ionos",
    "kamatera",
    "keycdn",
    "leapswitch",
    "lightsail",
    "limelight",
    "linode",
    "mnx",
    "netlify",
    "ovh",
    "scaleway",
    "stackpath",
    "sucuri",
    "upcloud",
    "verizonmedia",
    "vercel",
    "vultr",
    "wasabi"
  ],
  "devDependencies": {
    "@adaptivelink/iata": "0.1.4",
    "concurrently": "8.2.2",
    "isomorphic-fetch": "3.0.0",
    "lodash": "4.17.21",
    "prettier": "3.3.3",
    "puppeteer": "22.13.0",
    "rollup": "4.17.2",
    "@rollup/plugin-terser": "0.4.4"
  },
  "scripts": {
    "build": "npx rollup -c",
    "bun": "bun build ./src/main.js --outfile ./dist/bundle.js --minify --target browser --format esm",
    "prebuild": "npx prettier --write '**/*.{js,json}' '!dist/**' --arrow-parens avoid --trailing-comma none",
    "iata": "node ./tools/iata-filter.js > ./data/iata-filtered-test.js && mv ./data/iata-filtered-test.js ./data/iata-filtered.js",
    "validate": "node ./tools/validator.js",
    "test": "node ./test/index.test.mjs",
    "csv": "node ./tools/csv-generator.js > ./notebooks/data.csv",
    "spotter-atlatincnet": "node ./tools/spotters/atlanticnet.js",
    "spotter-cdn77": "node ./tools/spotters/cdn77.js",
    "spotter-cloudflare": "node ./tools/spotters/cloudflare.js",
    "spotter-deno": "node ./tools/spotters/deno.js",
    "spotter-digitalocean": "node ./tools/spotters/digitalocean.js",
    "spotter-ec2": "node ./tools/spotters/ec2.js",
    "spotter-fastly": "node ./tools/spotters/fastly.js",
    "spotter-imperva": "node ./tools/spotters/imperva.js",
    "spotter-keycdn": "node ./tools/spotters/keycdn.js",
    "spotter-lightsail": "node ./tools/spotters/lightsail.js",
    "spotter-linode": "node ./tools/spotters/linode.js",
    "spotter-stackpath": "node ./tools/spotters/stackpath.js",
    "spotter-upcloud": "node ./tools/spotters/upcloud.js",
    "spotter-vercel": "node ./tools/spotters/vercel.js",
    "spotter-vultr": "node ./tools/spotters/vultr.js",
    "spotters": "concurrently \"yarn:spotter-*\""
  }
}
